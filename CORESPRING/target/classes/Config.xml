<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:p="http://www.springframework.org/schema/p"      
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
 <!-- first bean -->
   <bean class="com.springcore.Student" name="Student1">
 <!--First property-->
 <property name="studentId">
 <value>12345</value>
 </property>

 <!--Second property-->
 <property name="studentName">
 <value>Diyajain</value>
 </property>

 <!--Third property-->
 <property name="studentAddress">
 <value>Delhi</value>
 </property>
   </bean>
 
<!-- Second bean -->
   <bean class="com.springcore.Student" name="Student2">
 <!--First property-->
<property name="studentId" value="678"/>

 <!--Second property-->
<property name="studentName" value="Charujain"/>
 
 <!--Third property-->
<property name="studentAddress" value="Mumbai"/>
  </bean>
  
   
<!-- Third bean -->
   <bean class="com.springcore.Student" name="Student3" p:studentId="910" p:studentName="samyakjain" p:studentAddress="chennai"/>
</beans>