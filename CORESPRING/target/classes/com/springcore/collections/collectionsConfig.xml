<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:p="http://www.springframework.org/schema/p"      
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
 <!-- first bean -->
   <bean class="com.springcore.collections.Emp" name="Employee1">
 <!--First property-->
 <property name="name" value="priyanka"/>

 <!--Second property-->
 <property name="address">
 <list>
 <value>Delhi</value>
 <value>mumbai</value>
 <value>chennai</value>
 <value>Delhi</value>
 <null/>
 </list>
 </property>

 <!--Third property-->
 <property name="phones">
 <set>
 <value>1234</value>
 <value>567</value>
 <value>8910</value>
 <value>786</value>
  <value>1294</value>
 <value>5670</value>
 <value>8980</value>
 <value>786</value>
 </set>
 </property>
 
 <!--Fourth property-->
 <property name="courses">
 <map>
 <entry key="java" value="1.2months"/>
 <entry key="python" value="3.2months"/>
 <entry key="dsa" value="8.2months"/>
 </map>
 </property>
 
 <!--Fifth property-->
 <property name="props">
 <props>
 <prop key="science">99</prop>
 <prop key="math">98</prop>
 <prop key="physics">100</prop>
 </props>
 </property>
   </bean>

</beans>